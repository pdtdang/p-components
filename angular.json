{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "p-button": {
      "projectType": "library",
      "root": "projects/p-button",
      "sourceRoot": "projects/p-button/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/p-button/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/p-button/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/p-button/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/p-button/src/test.ts",
            "tsConfig": "projects/p-button/tsconfig.spec.json",
            "karmaConfig": "projects/p-button/karma.conf.js"
          }
        }
      }
    },
    "p-dialog": {
